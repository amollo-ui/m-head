{
  "name": "m-head",
  "version": "0.0.1",
  "description": "Head manager for React",
  "author": "arma73 <arma.biz9805@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amollo-ui/m-head"
  },
  "keywords": [
    "head",
    "tags",
    "meta",
    "title",
    "react-title",
    "react-meta-tags",
    "react-head",
    "component",
    "components",
    "frontend",
    "react",
    "markup"
  ],
  "bugs": {
    "url": "https://github.com/amollo-ui/m-head/issues"
  },
  "homepage": "https://github.com/amollo-ui/m-head#README.md",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.{ts,tsx}\"",
    "lint": "eslint \"*/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint \"*/**/*.{js,ts,tsx}\" --quiet --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.16",
    "@babel/eslint-plugin": "^7.12.13",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  }
}
